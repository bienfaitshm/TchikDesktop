{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/*",
    "src/preload/*",
    "src/main/**/*",
    "src/commons/*",
    "src/commons/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
    "paths": {
      "@/main/*": ["./src/main/*"],
      "@/commons/*": ["./src/commons/*"]
    }
  }
}
